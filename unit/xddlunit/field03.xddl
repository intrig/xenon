<?xml version="1.0" encoding="UTF-8" ?>
<xddl>
  <!-- 
    Here, the record length is forced to be 60 bits, but only 5 bits are 
    defined.  
   -->
  <start>
    <field name="a" length="1"/>
    <record name="b" length="60">
      <field name="b1" length="2"/>
      <field name="b2" length="3"/>
    </record>
    <field name="c" length="8"/>
  </start>
</xddl>
