<xddl>
  <start>
    <field name="length_in_bytes" length="8"/>
    <field name="length" length="8"/>
    <field name="count" length="8"/>
    <record name="data" length="length*(length_in_bytes?8 : 1)">
      <repeat name="repeat" num="count">
        <field name="item" length="8"/>
      </repeat>
    </record>
  </start>
</xddl>
