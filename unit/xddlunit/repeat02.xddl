<?xml version="1.0" encoding="UTF-8" ?>
<xddl>
  <!-- variable repeat -->
  <start>
    <field name="length" length="8"/>
    <!-- this is actually the repeat 
    count, not length, oh well -->
    <repeat name="repeat" num="length">
      <field name="a" length="4"/>
      <field name="b" length="4"/>
    </repeat>
  </start>
</xddl>
