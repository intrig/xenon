<xddl>
  <comment>A simple expression with spaces in it</comment>
  <start>
    <field name="a b" length="8"/>
    <field name="b" length="{a b}"/>
    <prop name="another prop" value="{a b} + {a b}"/>
    <prop name="again prop" value="{another prop} + {a b} + {a b}"/>
  </start>
</xddl>
