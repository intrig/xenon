<xddl>
  <start>
    <oob>
      <field name="Timestamp" length="8"/>
      <field name="Channel" length="8"/>
    </oob>
    <switch expr="Channel">
      <case value="1">
        <field name="ch1 data" length="8"/>
      </case>
      <case value="2">
        <field name="ch2 data" length="8"/>
      </case>
    </switch>
  </start>
</xddl>
