<xddl>
  <type name="Magic Number" id="MagicNumber">
    <script>
    description = string.format("%08X", key)
  </script>
  </type>

  <type id="ascii">
    <script>
    description = string.format("%s", ascii(key))
  </script>
  </type>

</xddl>
