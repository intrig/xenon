  <!-- TYPES -->
  <!-- misc types -->
  <type id="zero">
    <item key="0" value=" "/>
    <!-- all other values are invalid -->
  </type>
  <type id="one">
    <range start="1" end="1"/>
  </type>
  <type id="Boolean">
    <item key="0" value="false"/>
    <item key="1" value="true"/>
  </type>
  <type id="IE_Included">
    <item key="0" value="Not included"/>
    <item key="1" value="Included"/>
  </type>
  <type id="IE_Included_Inverted">
    <item key="1" value="Not included"/>
    <item key="0" value="Included"/>
  </type>
  <type id="IE_Continued">
    <item key="0" value="Included"/>
    <item key="1" value="End of List"/>
  </type>
  <type name="To Be Determined" id="TBD">
    <item key="0" value="To Be Determined"/>
  </type>
  <!-- Formats -->
  <type id="Alpha">
    <script>
      description = string.format("%d.%d dBm", (key &lt; 10) and 0 or 1, (key &lt; 10) and key or 0)
    </script>
  </type>
  <type id="Gamma">
    <script>
     description = string.format("%d dB", key * 2)
   </script>
  </type>
  <type id="EGPRS Window Size">
    <script>
     description = string.format("%s", (key == 31) and 'Reserved' or (key + 2)*32)
   </script>
  </type>
