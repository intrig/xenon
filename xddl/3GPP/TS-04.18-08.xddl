                  <case value="0">
                    <!-- Radio Blocks -->
                    <repeat name="ALLOCATION_BITMAP">
                      <field name="block period" length="1" type="#10.5.2.25c.1fa"/>
                    </repeat>
                  </case>
                  <case value="1">
                    <!-- Radio Block Periods -->
                    <repeat name="ALLOCATION_BITMAP">
                      <field name="block period" length="1" type="#10.5.2.25c.1fb"/>
                    </repeat>
                  </case>
                </switch>
              </case>
            </switch>
